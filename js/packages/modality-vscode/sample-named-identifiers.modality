// Sample file to test named identifier highlighting

model TrafficLight {
  part RedLight {
    off --> on: +turnRed
    on --> off: +turnOff
  }
  
  part GreenLight {
    off --> on: +turnGreen
    on --> off: +turnOff
  }
  
  part YellowLight {
    off --> on: +turnYellow
    on --> off: +turnOff
  }
}

formula TrafficFlow {
  <+turnRed> true and
  <+turnGreen> true and
  <+turnYellow> true
}
