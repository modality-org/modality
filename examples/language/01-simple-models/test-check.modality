model TestModel1:
  part g1:
    n1 --> n2: +blue
    n2 --> n3: +green
    n3 --> n1: +red

model TestModel2:
  part g1:
    n1 --> n2: +blue
    n2 --> n3: +green
  part g2:
    n1 --> n1: +yellow

formula FormulaBlue: <+blue> true
formula FormulaGreen: <+green> true
formula FormulaYellow: <+yellow> true
formula FormulaBlueAndGreen: <+blue +green> true
formula FormulaComplex: <+blue> <+green> true 